VertexLitGeneric
{

	"Proxies"
	{
		"PaintOverlayColor"
		{
			"paintpower"	"1"
			"resultVar"		"$color2"
		}
	}

// Fallbacks for if colour proxy doesn't work
$usecolor 1 // This MUST be set for $color and $alpha to work!
$color "[ 0 0 0 ]"
$alpha "1"
$basetexture "paint/bridge_paint_single"
$detail "paint/bridge_paint_selfillum_stick"
$detailblendmode 5
$detailscale 1
$bumpmap "paint/bridge_paint_normal"
$envmap "paint/paint_envmap_hdr"
$vertexcolor 1
$translucent 1
$envmaptint "[ .08 .08 .08]"
$phong 1
$phongtint "[0 0 0]"
$surfaceprop glass
"%keywords" Portal2
$nocull 1 
$selfillumfresnel 1
$decal 1
}